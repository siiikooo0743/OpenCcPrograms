local tank = peripheral.wrap("top")

while true do
  sleep(0.2)
  local tableInfo = tank.getTanks("unknown")
  local amount
  for key, value in pairs(tableInfo) do
    if(value.amount == nil) then
      amount = 0
    else
      amount = value.amount
    end
  end
  
  print(amount)
  if(amount > 1000) then
    rs.setBundledOutput("right", 1)
  else
    rs.setBundledOutput("right", 0)
  end
  
end
